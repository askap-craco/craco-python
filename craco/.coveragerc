[run]
branch = True
source =
    craco

[paths]
source =
    src/craco
    **/site-packages/craco

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
title = craco Coverage Test Report
directory = docs/source/_static/coverage
